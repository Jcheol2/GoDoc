<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginEnd="3dp"
    android:layout_marginRight="3dp"
    android:background="@drawable/godoc_background"
    tools:context=".Page_19mypage">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="613dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="10dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_white_back">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/textView1"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginLeft="10dp"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="올리브내과병원"
                                    android:textColor="#000000"
                                    android:textSize="24sp" />

                                <TextView
                                    android:id="@+id/textView2"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginEnd="10dp"
                                    android:layout_marginRight="10dp"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="2022년 5월 14일 2시 30분"
                                    android:textSize="14sp" />
                            </LinearLayout>

                        </LinearLayout>

                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/shape_white_back" >

                            <Button
                                android:id="@+id/hos_write"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="전자문진표 작성"
                                android:textColor="#000000"
                                android:textSize="12sp"
                                app:backgroundTint="#FFFFFF" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/shape_white_back" >

                            <Button
                                android:id="@+id/hos_view_1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="예약 시간 변경"
                                android:textColor="#000000"
                                android:textSize="12sp"
                                app:backgroundTint="#FFFFFF" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/shape_white_back" >

                            <Button
                                android:id="@+id/review_1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="예약 취소"
                                android:textColor="#000000"
                                android:textSize="12sp"
                                app:backgroundTint="#FFFFFF" />
                        </RelativeLayout>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="10dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_white_back">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/hos_name"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginLeft="10dp"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="조이이비인후과병원"
                                    android:textColor="#000000"
                                    android:textSize="26sp" />

                                <TextView
                                    android:id="@+id/textView4"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginEnd="10dp"
                                    android:layout_marginRight="10dp"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="2022년 5월 12일 4시 30분" />
                            </LinearLayout>

                        </LinearLayout>
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/shape_white_back" >

                            <Button
                                android:id="@+id/prescription_2"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="전자문진표 작성"
                                android:textColor="#000000"
                                android:textSize="12sp"
                                app:backgroundTint="#FFFFFF" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/shape_white_back" >

                            <Button
                                android:id="@+id/hos_view_2"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="예약 시간 변경"
                                android:textColor="#000000"
                                android:textSize="12sp"
                                app:backgroundTint="#FFFFFF" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/shape_white_back" >

                            <Button
                                android:id="@+id/review_2"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="예약 취소"
                                android:textColor="#000000"
                                android:textSize="12sp"
                                app:backgroundTint="#FFFFFF" />
                        </RelativeLayout>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="10dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_white_back">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/textView5"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginLeft="10dp"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="예시티치과병원"
                                    android:textColor="#000000"
                                    android:textSize="26sp" />

                                <TextView
                                    android:id="@+id/textView6"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginEnd="10dp"
                                    android:layout_marginRight="10dp"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="2022년 5월 4일 10시 30분" />
                            </LinearLayout>

                        </LinearLayout>

                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/shape_white_back" >

                            <Button
                                android:id="@+id/prescription_3"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="전자문진표 작성"
                                android:textColor="#000000"
                                android:textSize="12sp"
                                app:backgroundTint="#FFFFFF" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/shape_white_back" >

                            <Button
                                android:id="@+id/hos_view_3"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="예약 시간 변경"
                                android:textColor="#000000"
                                android:textSize="12sp"
                                app:backgroundTint="#FFFFFF" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:background="@drawable/shape_white_back" >

                            <Button
                                android:id="@+id/review_3"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="예약 취소"
                                android:textColor="#000000"
                                android:textSize="12sp"
                                app:backgroundTint="#FFFFFF" />
                        </RelativeLayout>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <ImageView
                android:id="@+id/banner10"
                android:layout_width="match_parent"
                android:layout_height="123dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/banner"
                tools:srcCompat="@drawable/banner" />

        </LinearLayout>
    </ScrollView>

</LinearLayout>